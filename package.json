    "@vue/cli-plugin-babel": "^4.4.4",
    "autoprefixer": "^9.8.1",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
